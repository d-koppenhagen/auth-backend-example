    "dotenv-webpack": "^1.5.7",
    "express": "^4.16.3",
    "jwt-simple": "^0.5.1",
    "morgan": "^1.9.0",
    "webpack-cli": "^3.1.0",
    "winston": "^3.0.0",
    "winston-syslog": "^2.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/jwt-simple": "^0.5.33",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.5",
    "@types/winston": "^2.3.9",
    "@types/winston-syslog": "^1.0.1",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.4",
    "webpack-node-externals": "^1.7.2"
